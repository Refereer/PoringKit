{
  "constants": {
    "allianz_color": "#FA8B98",
    "rounded_corner": 20
  },
  "shared": {
    "font_family": "avenir",
    "segmented_control": {
      "tint_color": "$allianz_color"
    },
    "page_control": {
      "color": "light_gray",
      "current_color": "$allianz_color"
    },
    "switch": {
      "active_color": "$allianz_color",
      "thumb_color": "#EEE"
    },
    "navigation_bar": {
      "bar_color": "$allianz_color",
      "translucent": false,
      "font_family": "avenir",
      "font_style": "medium",
      "font_size": 17,
      "large_font_size": 38,
      "tint_color": "white",
      "text_color": "#FFFC"
    },
    "toolbar": {
      "tint_color": "white"
    },
    "bar_button": {
      "font_family": "avenir",
      "font_size": 17
    },
    "toolbar": {
      "tint_color": "white",
      "bar_color": "$allianz_color",
      "translucent": false
    },
    "text_view": {
      "font_family": "avenir",
      "font_size": 13,
      "border_style": "line",
      "border_width": 1,
      "border_color": "$allianz_color",
      "corner_radius": "$rounded_corner"
    },
    "text_field": {
      "font_family": "avenir",
      "font_size": 16,
      "border_style": "line",
      "border_width": 1,
      "border_color": "$allianz_color"
    }
  },
  "fonts": {
    "avenir": {
      "regular": "AvenirNext-Regular",
      "medium": "AvenirNext-Medium"
    }
  },
  "primary_button": {
    "corner_radius": "$rounded_corner",
    "text_color": "white",
    "font_family": "avenir",
    "background_color": "$allianz_color"
  },
  "ghost_button": {
    "border_color": "$allianz_color",
    "border_width": 2,
    "corner_radius": "$rounded_corner",

    "font_family": "avenir",
    "text_color": "$allianz_color",
    "text_color_disable": "$allianz_color",
    "background_color": "clear"
  },
  "general_input": {
    "keyboard_type": "phone_pad",
    "text_alignment": "center",
    "custom": "Test Text"
  },
  "title_text": {
    "font_size": 22,
    "font_style": "medium",
    "text_color": "$allianz_color"
  },
  "detail_text": {
    "font_size": 16,
    "text_color": "$allianz_color"
  },
  "round_image_view": {
    "corner_radius": "$rounded_corner",
    "background_color": "light_gray"
  }
}
